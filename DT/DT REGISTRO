 select edi_direccion from edificio
 where edi_cp= CAMPO CP  ///AGREGAR CAMPO CP A EDIFICIO Y COLONIA Y DATOS DE DOMICIOLIO

 Se dispara un trigger insertando un datos en la tabla DEPARTAMENTOS

INSERT INTO deptoper
values(Departamento_dep_idDepartamento,
 Departamento_Edificio_edi_idEdificio,
 Persona_per_idPersona
)

